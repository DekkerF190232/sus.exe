
package Main {

  use sus/InOut;
  use sus/System;
  use mogroft/Window;

  main {

    int32 r = add(a: 5, b: 10 + add(a: 2, b: 3));

    InOut/print(s: STR"added: ");
    InOut/print(i: r);
    InOut/printLn();

    InOut/print(s: STR"const: ");
    InOut/print(i: Window/ERR_DC);
    InOut/printLn();

    InOut/print(s: STR"DONE.\n");

    System/exit(code: 0);
    
  }

  func int32 add(int32 a, int32 b) {
    done a + b;
  }

}